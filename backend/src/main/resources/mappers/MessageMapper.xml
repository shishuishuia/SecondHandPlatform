<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.shishuishuia.mapper.MessageMapper">

    <select id="getMessageBySenderId" resultType="com.shishuishuia.pojo.Message">
        select *
        from message
        where senderId = #{id}
    </select>
</mapper>